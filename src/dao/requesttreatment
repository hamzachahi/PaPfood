DROP TABLE IF EXISTS papfood.administrator;
CREATE TABLE  papfood.administrator (
  Id int(10) unsigned NOT NULL AUTO_INCREMENT,
  name varchar(45) NOT NULL,
  second_name varchar(45) NOT NULL,
  surname varchar(45) NOT NULL,
  second_surname varchar(45) NOT NULL,
  profession varchar(45) NOT NULL,
  date_inscription datetime NOT NULL,
  password varchar(45) NOT NULL,
  email varchar(45) NOT NULL,
  phone_number varchar(45) NOT NULL,
  tel_number varchar(45) NOT NULL,
  facebook_id varchar(45) NOT NULL,
  twitter_id varchar(45) NOT NULL,
  instagram_id varchar(45) NOT NULL,
  linkedin_id varchar(45) NOT NULL,
  account_picture blob NOT NULL,
  street_number int(10) unsigned NOT NULL,
  street_name varchar(45) NOT NULL,
  city_name varchar(45) NOT NULL,
  country_name varchar(45) NOT NULL,
  postal_code varchar(45) NOT NULL,
  last_connexion int(10) unsigned NOT NULL,
  PRIMARY KEY (Id),
  CONSTRAINT last_connexion_2 FOREIGN KEY (Id) REFERENCES connexion (Id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;